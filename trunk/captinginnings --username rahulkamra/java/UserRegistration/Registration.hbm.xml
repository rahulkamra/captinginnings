<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="UserRegistration.RegistrationPojo" optimistic-lock="version" polymorphism="implicit" select-before-update="false" table="CAPTING_REG">
   <id name="teamname" column="teamname" >
            <generator class="assigned"/>
        </id>
        <property name="password"/>
        <property name="firstname"/>
        <property name="lastname" />
        <property name="security_question" column="security_question" />
        <property name="security_answer" column="security_answer"/>
        <property name="location" />
        <property name="username" column="username" />
               
  
  
  </class>
</hibernate-mapping>
